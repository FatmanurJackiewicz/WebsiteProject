<section class="ftco-section contact-section ftco-no-pb" id="contact-section">
	<div class="container">
		<div class="row justify-content-center mb-5 pb-3">
			<div class="col-md-7 heading-section text-center ftco-animate">
				<h1 class="big big-2">Contact</h1>
				<h2 class="mb-4">Contact Me</h2>
				<p>Empowering your business with creative and effective solutions.</p>
			</div>
		</div>

		<div class="row d-flex contact-info mb-5">
			<div class="col-md-6 col-lg-3 d-flex ftco-animate">
				<div class="align-self-stretch box p-4 text-center">
					<div class="icon d-flex align-items-center justify-content-center">
						<a href="https://www.google.com/search?q=london%2Cuk&oq=london%2Cuk&gs_lcrp=EgZjaHJvbWUyBggAEEUYOTIHCAEQABiABDIHCAIQABiABDIHCAMQABiABDIHCAQQABiABDIHCAUQABiABDIGCAYQRRhBMgYIBxBFGEEyBggIEEUYQdIBCTM2NzEyajBqNKgCALACAA&sourceid=chrome&ie=UTF-8" target="_blank"><span class="icon-map-signs"></span></a>
					</div>
					<h3 class="mb-4">Address</h3>
					<a href="https://www.google.com/search?q=london%2Cuk&oq=london%2Cuk&gs_lcrp=EgZjaHJvbWUyBggAEEUYOTIHCAEQABiABDIHCAIQABiABDIHCAMQABiABDIHCAQQABiABDIHCAUQABiABDIGCAYQRRhBMgYIBxBFGEEyBggIEEUYQdIBCTM2NzEyajBqNKgCALACAA&sourceid=chrome&ie=UTF-8" target="_blank"><p>London, UK</p></a>
				</div>
			</div>
			<div class="col-md-6 col-lg-3 d-flex ftco-animate">
				<div class="align-self-stretch box p-4 text-center">
					<div class="icon d-flex align-items-center justify-content-center">
						<a href="https://github.com/FatmanurJackiewicz" target="_blank"><span class="icon-github"></span></a>
					</div>
					<h3 class="mb-4">GitHub</h3>
					<p><a href="https://github.com/FatmanurJackiewicz" target="_blank"> Fatmanur's GitHub </a></p>
				</div>
			</div>
			<div class="col-md-6 col-lg-3 d-flex ftco-animate">
				<div class="align-self-stretch box p-4 text-center">
					<div class="icon d-flex align-items-center justify-content-center">
						<span class="icon-paper-plane"></span>
					</div>
					<h3 class="mb-4">Email Address</h3>
					<p><a href="mailto:fatmanurjackiewicz@gmail.com">info@fatmanurjackiewicz.com</a></p>
				</div>
			</div>
			<div class="col-md-6 col-lg-3 d-flex ftco-animate">
				<div class="align-self-stretch box p-4 text-center">
					<div class="icon d-flex align-items-center justify-content-center">
						<span class="icon-globe"></span>
					</div>
					<h3 class="mb-4">Website</h3>
					<p><a href="#">fatmanurJackiewicz.com</a></p>
				</div>
			</div>
		</div>

		<div class="row no-gutters block-9">
			<div class="col-md-6 order-md-last d-flex pl-4">
				@await Component.InvokeAsync("ContactForm")
			</div>

			<div class="col-md-6 d-flex">
				<div class="img" style="background-image: url(@ViewBag.ImageUrl2);"></div>
			</div>
		</div>
	</div>
</section>
